page { 
  height: 100%;
  background-image: linear-gradient(45deg, #dc9fb4, #33a6b8);
}

.avatar {
  display: block;
  border-radius: 50%;
  width: 100rpx;
  height: 100rpx;
  overflow: hidden;
  margin: 10rpx;
}

.cetBlackground{
  height: 100%;
  width: 100%;
  position: fixed;
}

.cetCard{
  background-color: #fff;
  width: 80%;
  border-radius: 2em;
  box-shadow: 0 3rem 100px rgba(0, 0, 0, 0.1);
  position: relative;
}

.testDate{
  position: absolute;
  bottom: 0;
  right: 0;
  color:  rgb(90, 90, 90);
}

.line{
  width: 80%;
  height: 5rpx;
  background-color: rgba(0, 0, 0, 0.1);
}

.scoreLine{
  width: 5rpx;
  height: 200rpx;
  background-color: rgba(0, 0, 0, 0.1);
}

.cetScore {
  font-family: Georgia, "Times New Roman", Times, serif;
  font-size: 200rpx;
  background-image: linear-gradient(45deg, #dc9fb4, #33a6b8);
  background-clip: text;
  -webkit-background-clip: text;
  color: transparent;
}

.cetSwiper{
  width: 100%;
  height: 62% !important;
}

.captchaWraper {
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 999;
  background-color: #fff;
}

.captcha {
  position: absolute;
  width: 60% !important;
  height: 300rpx;
  transform: translateY(50%) scale(1);
}

.captcha .img {
  width: 90%;
  height: 80%;
  /* height: 298rpx; */
  border: solid 5rpx transparent;
}

.captcha .img > image{
  width: 100%;
  height: 100%;
}

.captcha .inputWraper{
  border: solid 1rpx #b4b4b4;
  border-radius: 2em;
  width: 80%;
}

.captcha .submitbtn{
  color: #fff;
  width: 20%;
  height: 100%;
  border-radius: 2em;
  background-image: linear-gradient(45deg, #dc9fb4, #33a6b8);
  box-shadow: 5px 5px 20px rgb(209, 209, 209);
  transition: all .5s;
}

.captcha .hover{
  opacity: 0.9;
  transform: translate(3rpx,3rpx) scale(0.95, 0.95);
  box-shadow:0px 0px 8px rgba(0, 0, 0, .1) inset; 
  transition: all .5s;
}

.captcha .imgHover{
  border: solid 5rpx rgb(150, 149, 149);
}

.cetRefresh{
  font-size: 50rpx;
  position: absolute;
  right: 0;
  top: 0;
  color: #aaa;
}